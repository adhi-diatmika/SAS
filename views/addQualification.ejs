<%- include partials/headerMainSAS %>
<div class="maindiv">
    <div>
        <div class="window">
            <!-- <a href="#" class="closebutton" title="Close">X</a> -->
            <h3>Add Qualification</h3>
            <form  action="/addQualification" method="POST">
                <h4>Qualification Name</h4>
                <input type="text" name="qualificationName" id="qualificationName" placeholder="Name" class="inputform">
                <h4>Minimum score</h4>
                <input type="number" name="minimumScore" id="minimumScore" placeholder="Minimum score" class="inputform">
                <h4>Maximum Score</h4>
                <input type="number" name="maximumScore" id="maximumScore" placeholder="Maximum score" class="inputform">
                <h4>Grade List</h4>
                <textarea name="gradeList" id="gradeList" placeholder="Grade List" class="inputform"></textarea>
                <h4>Result Description</h4>
                <textarea name="resultDescription" id="resultDescription" placeholder="Result Description" class="inputform"></textarea>
                <button type="submit" class="submitbutton">Submit</button>
            </form>
        </div>
    </div>
</div>
<%- include partials/footerSAS %>