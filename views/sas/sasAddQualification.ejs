<%- include ../partials/headerMainSAS %>
<div class="maindiv">
    <div>
        <div class="window">
            <!-- <a href="#" class="closebutton" title="Close">X</a> -->
            <h3>Add Qualification</h3>
            <form action="/sas/add" method="POST">
                <h4>Qualification Name</h4>
                <input type="text" name="qualificationName" placeholder="Name" class="inputform">
                <h4>Minimum score</h4>
                <input type="number" name="minimumScore" placeholder="Minimum score" class="inputform" onkeydown='return (event.which >= 48 && event.which <= 57) || event.which == 8 || event.which == 46'>
                <h4>Maximum Score</h4>
                <input type="number" name="maximumScore" placeholder="Maximum score" class="inputform" onkeydown='return (event.which >= 48 && event.which <= 57) || event.which == 8 || event.which == 46'>
                <h4>Grade List</h4>
                <textarea name="gradeList" id="exampleFormControlTextarea1" rows="3" placeholder="Grade List" class="inputform"></textarea>
                <h4>Result Description</h4>
                <textarea name="resultCalcDesc" id="exampleFormControlTextarea1" rows="3" placeholder="Result Description" class="inputform"></textarea>
                <input type="submit" class="submitbutton" value="Submit">
                <button class="submitbutton"><a href="/sas/maintain">Cancel</a></button>
            </form>
        </div>
    </div>
</div>
<%- include ../partials/footerSAS %>