<%- include partials/headerMainSAS %>
<div class="maindiv">
    <nav>
        <aside class="sideleft">
            <div><a href="/addQualification">+ Add Qualification</a></div>
        </aside>
    </nav>
    <nav>
        <aside class="sideright"><h1>Qualification List</h1>
            <div class="searchbar">
                <input type="text" placeholder="Search" class="search" required>
                <input type="button" value="Search" class="buttonsearch">
            </div>
            <table cellspacing='0'>
                <thead>
                    <tr>
                        <th>No</th>
                        <th>Qualification Name</th>
                        <th>Maximum Score</th>
                        <th>Status</th>
                    </tr>
                </thead>
                <tbody>
                    <% if(qualificationList.length > 0) { let i=1; %>
                        <% qualificationList.forEach(function(qualification) { %>
                            <tr>
                                <th><%= i %></th>
                                <td><%= qualification.qualificationName %></td>
                                <td><%= qualification.maximumScore %></td>
                                <td>
                                    <img src="./Logo/Edit.png" alt="Edit" class="tableimg">
                                    <img src="./Logo/Delete.png" alt="Delete" class="tableimg">
                                </td>                                    
                            </tr>
                        <% i++; }) %>
                    <% } else { %>
                        <tr >
                            <td  colspan="5">No Data Found</td>
                          </tr>
                    <% } %>
                </tbody>
            </table>
        </aside>
    </nav>
</div>
<%- include partials/footerSAS %>